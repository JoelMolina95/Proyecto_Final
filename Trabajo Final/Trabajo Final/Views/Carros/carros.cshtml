@model List<Trabajo_Final.Models.Vehiculo>
@{
    ViewBag.Title = "carros";
}

<h2 style="color:red;">Estos son los vehiculos disponibles</h2>
<h4 style="color:blue;">¡Espero encuentres el que te intereso!</h4>
<table class="table table-striped">
    @foreach (var racer in Model)
    {

        <thead class="table-bordered">
        <th>   Id   </th>
        <th>   Marca     </th>
        <th>   Modelo   </th>
        <th>   Color    </th>
        <th>   NumSerial  </th>
        <th>   Precio_Venta  </th>
        <th>   Descripcion  </th>
        <th style="color:red;"> Acción </th>
        </thead>


        <tr>
            <td>@racer.IdVehiculo</td>
            <td>@racer.Marca </td>
            <td>@racer.modelo </td>
            <td>@racer.color </td>
            <td>@racer.NumSerial </td>
            <td>@racer.precio_venta </td>
            <td>@racer.descripcion </td>
            <td class="btn btn-success">@Html.ActionLink("Comprar", "Delete", new { id = racer.IdVehiculo }) </td>
        </tr>

    }
</table>
             
